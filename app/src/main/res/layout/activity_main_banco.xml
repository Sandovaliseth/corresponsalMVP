<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".view.Banco.MainBActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="204dp"
        android:layout_height="166dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="-50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/circulop" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="166dp"
        android:layout_height="150dp"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="336dp"
        android:rotation="-10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/circulop" />

    <TextView
        android:id="@+id/txtNombreC"
        android:layout_width="242dp"
        android:layout_height="69dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="160dp"
        android:layout_marginBottom="500dp"
        android:text="Nombre Administrador"
        android:textColor="@color/orange"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toStartOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnCrearCli"
        android:layout_width="118dp"
        android:layout_height="134dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/btn_redondeado"
        android:drawableTop="@drawable/tarjeta"
        android:padding="16dp"
        android:text="Crear cliente"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNombreC" />

    <Button
        android:id="@+id/btnCrearCo"
        android:layout_width="118dp"
        android:layout_height="134dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="47dp"
        android:background="@drawable/btn_redondeado"
        android:drawableTop="@drawable/retiro"
        android:padding="16dp"
        android:text="Registrar corresponsal"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnCrearCli"
        app:layout_constraintTop_toBottomOf="@+id/txtNombreC" />

    <Button
        android:id="@+id/btnConsultarCli"
        android:layout_width="118dp"
        android:layout_height="134dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/btn_redondeado"
        android:drawableTop="@drawable/depositar"
        android:padding="16dp"
        android:text="Consultar cliente"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCrearCli" />

    <Button
        android:id="@+id/btnConsultarCo"
        android:layout_width="118dp"
        android:layout_height="134dp"
        android:layout_marginStart="77dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="51dp"
        android:background="@drawable/btn_redondeado"
        android:drawableTop="@drawable/transferir"
        android:padding="16dp"
        android:text="Consultar corresponsal"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnConsultarCli"
        app:layout_constraintTop_toBottomOf="@+id/btnCrearCo" />

    <Button
        android:id="@+id/btnListaCli"
        android:layout_width="118dp"
        android:layout_height="134dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="33dp"
        android:layout_marginBottom="9dp"
        android:background="@drawable/btn_redondeado"
        android:drawableTop="@drawable/expediente"
        android:padding="16dp"
        android:text="Listado clientes"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnConsultarCo" />

    <Button
        android:id="@+id/btnListaCo"
        android:layout_width="118dp"
        android:layout_height="134dp"
        android:layout_marginStart="77dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="91dp"
        android:background="@drawable/btn_redondeado"
        android:drawableTop="@drawable/consultadatos"
        android:padding="16dp"
        android:text="Listado corresponsales"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toEndOf="@+id/btnListaCli"
        app:layout_constraintTop_toBottomOf="@+id/btnConsultarCo"
        app:layout_constraintVertical_bias="0.346" />

    <Button
        android:id="@+id/menuB"
        android:layout_width="50dp"
        android:layout_height="48dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/sinborde"
        android:drawableRight="@drawable/menu"
        android:onClick="mostrarMenu"
        app:layout_constraintStart_toEndOf="@+id/txtNombreC"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>